<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>net461;net462;netstandard2.0</TargetFrameworks>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Authors>Eric Zimmerman</Authors>
    <Company>Eric Zimmerman</Company>
    <Product>RegistryPluginBase</Product>
    <Description>RegistryPluginBase. Use this to create your own Registry Explorer plugins!</Description>
    <Copyright>Eric Zimmerman</Copyright>
    <PackageProjectUrl>https://github.com/EricZimmerman/RegistryPlugins</PackageProjectUrl>
    <RepositoryUrl>https://github.com/EricZimmerman/RegistryPlugins</RepositoryUrl>
    <AssemblyVersion>1.1.0.0</AssemblyVersion>
    <FileVersion>1.1.0.0</FileVersion>
    <Version>1.1.0.0</Version>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="ExtensionBlocks" Version="1.1.0" />
    <PackageReference Include="GuidMapping" Version="1.0.0" />
    <PackageReference Include="Registry" Version="1.1.0.5" />
  </ItemGroup>


</Project>